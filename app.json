{
  "expo": {
    "name": "LinkUup",
    "slug": "linkuup-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linkuup.mobile",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "LinkUup needs your location to show nearby businesses and help you find services.",
        "NSUserNotificationsUsageDescription": "LinkUup needs permission to send you booking reminders and updates."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000"
      },
      "package": "com.linkuup.mobile",
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.RECORD_AUDIO"
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "scheme": "linkuup",
    "extra": {
      "apiBaseUrl": "https://linkuup.com/api/v1",
      "eas": {
        "projectId": "6f9c22ec-f427-43fd-ac85-c4349e55d3ae"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you upload business images."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff"
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow LinkUup to access your microphone for video features."
        }
      ]
    ],
    "owner": "cetler74"
  }
}
